package com.pentalog.dto;

import java.math.BigDecimal;

import com.pentalog.utilities.Currency;

public class AccountDTO {
	
	private String accountNumberFrom;
	
	private Currency accountType;
	
	private BigDecimal balance;
	
	private String accountNumberTo;
	
	
	public AccountDTO(String accountNumberFrom, Currency accountType, BigDecimal balance, String accountNumberTo) {
		this.accountNumberFrom = accountNumberFrom;
		this.accountType = accountType;
		this.balance= balance;
		this.accountNumberTo=accountNumberTo;
	}


	public Currency getAccountType() {
		return accountType;
	}

	public void setAccountType(Currency accountType) {
		this.accountType = accountType;
	}

	public BigDecimal getBalance() {
		return balance;
	}

	public void setBalance(BigDecimal balance) {
		this.balance = balance;
	}
}
